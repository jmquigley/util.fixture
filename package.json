{
  "name": "util.fixture",
  "version": "0.0.64",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Test fixture library",
  "main": "./index.js",
  "types": "./index.d.ts",
  "private": false,
  "license": "MIT",
  "debug": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.fixture"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.fixture/issues"
  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.11",
    "lorem-ipsum": "^1.0.6",
    "rimraf": "^2.6.3",
    "string-template": "^1.0.0",
    "util.chdir": "0.0.37",
    "util.constants": "^0.0.16",
    "util.filelist": "0.0.32",
    "util.join": "0.0.21",
    "util.toolbox": "0.0.68",
    "util.wait": "0.0.33",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.4",
    "@types/lodash": "^4.14.119",
    "@types/lorem-ipsum": "^1.0.2",
    "@types/node": "^10.12.18",
    "@types/rimraf": "^2.0.2",
    "@types/string-template": "^1.0.2",
    "@types/uuid": "^3.4.4",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "typescript-babel-jest": "^1.0.5",
    "util.pkgwrap": "^0.0.94"
  },
  "pkgwrap": {
    "cleanup": [
      "index.d.ts",
      "index.js"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "notify": false,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "__tests__/helpers"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      "^.+\\.ts?$": "typescript-babel-jest"
    },
    "verbose": false
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage --jest",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "test": "pkgwrap --testing --jest"
  }
}
