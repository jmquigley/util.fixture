{
	"compileOnSave": true,
	"compilerOptions": {
		"declaration": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"module": "commonjs",
		"target": "es6"
	}
}
